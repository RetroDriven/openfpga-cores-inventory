---
layout: none
---
[
  {%- for repo in site.data.repos %}
  {%- for core in repo.cores %}
  {
    "repo": {
      "user": {{ repo.user | jsonify }},
      "project": {{ core.project | jsonify }}
    },
    "name": {{ core.name | jsonify }},
    "platform": {{ core.platform | jsonify }}
    {%- if core.allow_prerelease -%},{%- endif -%}
    {% if core.allow_prerelease %}
    "allow_prerelease": {{ core.allow_prerelease | jsonify }}
    {%- unless core.assets %},{% endunless %}
    {%- endif %}
    {%- if core.assets %}
    {{- "," }}
    "assets": {{ core.assets | jsonify }}
    {%- endif %}
  }
  {%- unless forloop.last %},{% endunless %}
  {%- endfor %}
  {%- unless forloop.last %},{% endunless %}
  {%- endfor %}
]
