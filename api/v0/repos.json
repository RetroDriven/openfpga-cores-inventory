---
layout: none
---
[
  {%- for repo in site.data.repos %}
  {
    "repo": {
      "user": {{ repo.user | jsonify }},
      "project": {{ repo.project | jsonify }}
    },
    "name": {{ repo.name | jsonify }},
    "platform": {{ repo.platform | jsonify }}
    {%- if repo.allow_prerelease -%},{%- endif -%}
    {% if repo.allow_prerelease %}
    "allow_prerelease": {{ repo.allow_prerelease | jsonify }}
    {%- unless repo.assets %},{% endunless %}
    {%- endif %}
    {%- if repo.assets %}
    {{- "," }}
    "assets": {{ repo.assets | jsonify }}
    {%- endif %}
  }
  {%- unless forloop.last %},{% endunless %}
  {%- endfor %}
]
